<p id="notice"><%= notice %></p>

<h1>Listing Descoverlies</h1>

<table>
  <thead>
    <tr>
      <th>Descovery time</th>
      <th>Type</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @descoverlies.each do |descoverly| %>
      <tr>
        <td><%= descoverly.descovery_time %></td>
        <td><%= descoverly.type %></td>
        <td><%= descoverly.description %></td>
        <td><%= link_to 'Show', descoverly %></td>
        <td><%= link_to 'Edit', edit_descoverly_path(descoverly) %></td>
        <td><%= link_to 'Destroy', descoverly, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Descoverly', new_descoverly_path %>
